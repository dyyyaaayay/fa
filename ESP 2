#include <stdio.h>
#include <stdlib.h>


int count (int arr[], int size) {
    int count = 0;
    
    for (int i = 1; i < size; i++) {
        if (arr[i] == arr[i-1]) {
            count++;
        }
    }
    return count;
}

int Max10 (int arr[], int size) {
    int max = 0;
    
    for (int i = 1; i < size; i++) {
        if (abs(arr[i]) > abs(arr[max])) {
            max = i;
        }
    }
    return max;
}

void Sign (int arr[], int size) {
    int max = Max10(arr, size);
    
    arr[max] = -arr[max];
}

void e11(int arr1[], int size1, int arr2[], int size2, int result[], int size3) {
    int i = 0, j = 0, k = 0;
    
    while (i < size1 && j < size2) {
        if (arr1[i] <= arr2[j]) {
            result[k++] = arr1[i++];
        } else {
            result[k++] = arr2[j++];} }
    
    while (i < size1) {
        result[k++] = arr1[i++];}
    
    while (j < size2) {
        result[k++] = arr2[j++]; }
}


void Sort(int arr[], int size) {
    for (int i = 0; i < size - 1; i++) {
        int min = i;
        for (int j = i + 1; j < size; j++) {
            if (arr[j] < arr[min]) {
                min = j;}}
        
        int temp = arr[min];
        arr[min] = arr[i];
        arr[i] = temp;
    }
}

